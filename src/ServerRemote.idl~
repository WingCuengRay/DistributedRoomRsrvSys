module RemoteInterface{

	typedef sequence<string> ListOfString;
	typedef sequence<boolean> ListOfBool;

	interface ServerRemote
	{
		ListOfString AddRecord(in string date, in unsigned short room, in ListOfString timeslots);
		ListOfBool DeleteRecord(in string date, in unsigned short room, in ListOfString timeslots);
		string Book(in string stu_id, in string campus, in string date, in unsigned short room, in string timeslots);
		boolean CancelBook(in string booking_id, in string stu_id);
		string GetAvailTimeSlots(in string date);
		string ChangeReservation(in string stu_id, in string booking_id, in string new_campus_name, 
										in string new_room_no, in string new_timeslot);
	};
};

